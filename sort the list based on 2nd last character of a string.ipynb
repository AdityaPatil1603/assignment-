    "#sort list based on 2nd last charachter of the string \n",
    "list=[]\n",
    "size=int(input(\"Enter the list length:\"))\n",
    "print(\"Enter the list value\")\n",
    "for a in range(size):\n",
    "    value=input()\n",
    "    list.append(value)\n",
    "print(list)\n",
    "list.sort(key=lambda x:x[-2])\n",
    "print(\"Sorted list is:\",list)"
   ]
